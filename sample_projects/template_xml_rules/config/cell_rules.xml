<?xml version="1.0" encoding="UTF-8"?>

<hypothesis_rulesets>

    <hypothesis_ruleset name="default">
        <behavior name="apoptosis">
            <decreasing_signals />
            <increasing_signals>
                <max_response>1e-4</max_response>
                <signal name="substrate">
                    <half_max>4</half_max>
                    <hill_power>2</hill_power>
                    <applies_to_dead>0</applies_to_dead>
                </signal>
            </increasing_signals>
        </behavior>
        <behavior name="cycle entry">
            <decreasing_signals>
                <max_response>1e-2</max_response>
                <signal name="substrate">
                    <half_max>1</half_max>
                    <hill_power>2</hill_power>
                    <applies_to_dead>0</applies_to_dead>
                </signal>
            </decreasing_signals>
        </behavior>
    </hypothesis_ruleset>

</hypothesis_rulesets>
